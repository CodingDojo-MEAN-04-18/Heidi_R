<html>
    <head>
        <title></title>
    </head>
    <body>
            <ul>
                <% users.forEach(function(user){ %>
                   <li>Name:<%- user.name; %> , Age:<%- user.age; %></li> 
                <% }); %>
            </ul>
        <form action = "/users" method = "post">
            <label>Name:</label><input type="text" name = "name">
            <label>Age:</label><input type = "text" name = "age">
            <input type = "submit">
        </form>
    </body>
</html>